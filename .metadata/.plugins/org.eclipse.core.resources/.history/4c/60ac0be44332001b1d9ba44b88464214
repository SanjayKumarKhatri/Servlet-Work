package com.springcore.javaconfig;

public class Emp {
	
	private Samosa samosa;
	
	
	
	public Samosa getSamosa() {
		return samosa;
	}



	public void setSamosa(Samosa samosa) {
		this.samosa = samosa;
	}



	public Emp(Samosa samosa) {
		super();
		this.samosa = samosa;
	}



	public void work() {
		this.samosa.display();
		System.out.println("Employee is working");
	}

}
